.ul .li-item {
  width: 200rpx;
  height: 60rpx;
  box-sizing: content-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 40rpx 0 0;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(90, 90, 90, 1);
  border-bottom: 0.5rpx solid rgba(213, 213, 214, 0.5);
  z-index: 1;
  background:#fff;

}
.ul .li-item::after {
  content: '';
  clear: both;
  display: block;
  overflow: hidden;

}
.ul{
  background: #fff;
  border-radius: 10rpx;
}
.ul .change{
  background: rgb(131, 29, 29);
  display: none;
}


.ul .li-item-child{
  padding-left:60rpx;
}
.ul .li-item .text {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(169, 169, 169, 1);
}
.menu-img{
  width: 33rpx;
  height: 33rpx;
}